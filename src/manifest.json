{
    "name": "Chrome Proxy",
    "version": "1.0.0",
    "manifest_version": 2,
    "incognito": "split",
    "permissions": [
        "proxy",
        "tabs",
        "unlimitedStorage",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "minimum_chrome_version": "22.0.0"
}